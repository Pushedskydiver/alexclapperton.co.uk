            <path fill="#000000" d="M254 35a6 6 0 1 0 12 0 6 6 0 1 0-12 0" />
        </svg>

        <picture class="devices__picture">
            <source type="image/webp" data-srcset="https:{{url}}?fm=webp">
            <img class="devices__picture-image lazyload__image lazyload"data-src="https:{{url}}" alt="{{projectData.deviceOneAlt}}" data-lazy>

            <noscript>
                <source type="image/webp" srcset="https:{{url}}?fm=webp">
                <img class="devices__picture-image" src="https:{{url}}" alt="{{projectData.deviceOneAlt}}">
            </noscript>
        </picture>
    </div>
</div>
