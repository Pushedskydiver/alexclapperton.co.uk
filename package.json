    "sass-loader": "^12.3.0",
    "stylelint": "^14.0.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.61.0",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-bundle-analyzer": "^4.5.0",
